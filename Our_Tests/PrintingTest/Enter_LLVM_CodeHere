@.intFormat = internal constant [4 x i8] c"%d\0A\00"
declare i32 @printf(i8*, ...)

define i32 @fn_fib(i32, i32) {
    ; Enter your code here
    ; then in the next 2 lines you shall change the 
    ; loading and/or the return value to receive 
    ; what you expecting

    %3 = load i32, i32* %t3
